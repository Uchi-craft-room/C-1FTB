<?xml version="1.0"?>

<PropertyList>

 <path>fuel-total.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PointerTip</object-name>   
  <object-name>PointerTip2</object-name>   
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
      <red>   0.7 </red>
      <green> 0.3 </green>
      <blue>  0.3 </blue>
    </emission>
 </animation>


    <animation>
        <type>rotate</type>
        <object-name>PointerCentre</object-name>
        <object-name>PointerTip</object-name>
        <property>/consumables/fuel/total-fuel-lbs</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>30000</ind><dep>270</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>PointerTip2</object-name>
        <property>/consumables/fuel/total-fuel-lbs</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>30000</ind><dep>10800</dep></entry>
        </interpolation>
        <center>
        <x-m> 0.006 </x-m>
        <y-m> 0.000 </y-m>
        <z-m>-0.017 </z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>


</PropertyList>










