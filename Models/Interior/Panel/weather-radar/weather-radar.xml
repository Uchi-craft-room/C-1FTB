<?xml version="1.0"?>
<PropertyList>

    <path>weather-radar.ac</path>

    <animation>
        <type>select</type>
        <object-name>off_srf</object-name>
        <condition>
            <equals>
                <property>systems/electrical/outputs/nav</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>


  <animation>
   <type>material</type>
        <object-name>knob</object-name>
     <emission> 
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        <red>   0.7 </red>
        <green> 0.3 </green>
        <blue>  0.3 </blue>
      </emission>
  </animation> 

  <animation>
   <type>material</type>
        <object-name>base</object-name>
     <emission> 
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        <red>   0.7 </red>
        <green> 0.7 </green>
        <blue>  0.7 </blue>
      </emission>
  </animation> 


</PropertyList>






