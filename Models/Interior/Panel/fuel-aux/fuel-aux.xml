<?xml version="1.0"?>

<PropertyList>

 <path>fuel-aux.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PointerTip-RH</object-name>
  <object-name>PointerTip-LH</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
      <red>   0.7 </red>
      <green> 0.3 </green>
      <blue>  0.3 </blue>
    </emission>
 </animation>


    <animation>
        <type>rotate</type>
        <object-name>PointerTip-LH</object-name>
        <property>/consumables/fuel/tank[3]/level-lbs</property>
        <interpolation>
            <entry><ind>   0</ind> <dep>  0</dep></entry>
            <entry><ind>3500</ind> <dep>290</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>PointerTip-RH</object-name>
        <property>/consumables/fuel/tank[4]/level-lbs</property>
        <interpolation>
            <entry><ind>   0 </ind> <dep> 0.0 </dep></entry>
            <entry><ind>3500 </ind> <dep> 290 </dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>










