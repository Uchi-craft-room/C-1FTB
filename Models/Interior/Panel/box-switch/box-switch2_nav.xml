<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

 <path>box-switch2_nav.ac</path>

  <animation>
    <type>material</type>
    <object-name>switch</object-name>
    <emission>
      <red>   0.7 </red>
      <green> 0.3 </green>
      <blue>  0.3 </blue>   
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>switch.001</object-name>
    <emission>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>   
    </emission>
  </animation>

  <animation>
    <type>select</type>
    <object-name>switch.001</object-name>
    <condition>
       <equals>
        <property>autopilot/hdg-set-knob</property>
        <value>4</value>
       </equals>  
    </condition>
  </animation>


</PropertyList>
