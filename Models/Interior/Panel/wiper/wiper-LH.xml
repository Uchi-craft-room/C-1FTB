<?xml version="1.0"?>

<PropertyList>

 <path>wiper.ac</path>


 <animation>
  <type>rotate</type>
  <object-name>wiper</object-name>
  <object-name>arm-A</object-name>
  <object-name>arm-B</object-name>
   <property>/controls/special/wiper-deg</property>
   <interpolation>
      <entry><ind>  0</ind><dep> 75 </dep></entry>
      <entry><ind>110</ind><dep> 0 </dep></entry>
   </interpolation>
  <center>
   <x-m> 0 </x-m>
   <y-m> 0 </y-m>
   <z-m> 0 </z-m>
  </center>
  <axis>
   <x> 0 </x>
   <y> 0 </y>
   <z>-1</z>
  </axis>
 </animation>


 <animation>
  <type>translate</type>
  <object-name>wiper</object-name>
  <object-name>arm-B</object-name>
   <property>/controls/special/wiper-deg</property>
   <interpolation>
      <entry><ind>  0</ind><dep> 0.17 </dep></entry>
      <entry><ind>110</ind><dep>   0 </dep></entry>
   </interpolation>
  <axis>
   <x> 1 </x>
   <y> 0 </y>
   <z> 0 </z>
  </axis>
 </animation>


 <animation>
  <type>rotate</type>
  <object-name>wiper</object-name>
   <property>/controls/special/wiper-deg</property>
   <interpolation>
      <entry><ind> 0</ind><dep> 18 </dep></entry>
      <entry><ind>110</ind><dep> 10 </dep></entry>
   </interpolation>
  <center>
   <x-m> 0.364 </x-m>
   <y-m> 0.000 </y-m>
   <z-m> 0.046 </z-m>
  </center>
  <axis>
   <x> 0 </x>
   <y> 0 </y>
   <z>-1</z>
  </axis>
 </animation>


</PropertyList>

