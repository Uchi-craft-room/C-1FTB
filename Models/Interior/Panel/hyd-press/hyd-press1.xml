<?xml version="1.0"?>

<PropertyList>

 <path>hyd-press1.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PointerTip</object-name>
  <object-name>PointerTip.001</object-name>
<object-name>PointerCentre</object-name>   
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
      <red>   0.7 </red>
      <green> 0.3 </green>
      <blue>  0.3 </blue>
    </emission>
 </animation>


 <animation> 
  <type>rotate</type>
  <object-name>PointerTip</object-name>
  <property>engines/engine[0]/n2</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>10</ind><dep>253</dep></entry>
   <entry><ind>100</ind><dep>270</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0.000</x-m>
   <y-m>0.000</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>  

 <animation> 
  <type>rotate</type>
  <object-name>PointerTip.001</object-name>
  <property>engines/engine[1]/n2</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>10</ind><dep>250</dep></entry>
   <entry><ind>100</ind><dep>272</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0.000</x-m>
   <y-m>0.000</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>
 
</PropertyList>

